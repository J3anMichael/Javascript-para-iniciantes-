import React from 'react';

const Bemvindo = (props) => {
  return (
    <div>
      <h2>Welcome! {props.nome}</h2>
      <h3>Tenho {props.idade} Anos</h3>
    </div>
  )
} 

function App() {
  return (
    <div className="App">
      Hello, world!
      <Bemvindo nome="Jean" idade="24"/>
      <Bemvindo nome="Lunara" idade="28"/>
    </div>
  );
}

export default App;
